<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NavigationView">
        <attr name="headerLayout" format="reference"/>
        <attr name="menu" format="reference"/>

        <attr name="itemIconTint" format="color"/>
        <attr name="itemTextColor" format="color"/>

        <attr name="itemTextAppearance" format="reference"/>

        <attr name="item_rippleColor" format="reference|color" />
        <attr name="item_rippleColorInverse" format="reference|color" />

        <attr name="item_rippleHotspot" format="boolean" />
        <attr name="item_rippleStyle" format="enum">
            <enum name="over" value="0" />
            <enum name="background" value="1" />
            <enum name="borderless" value="2" />
        </attr>
        <attr name="item_rippleRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="Ripple">



    </declare-styleable>
</resources>